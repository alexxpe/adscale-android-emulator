{
  "name": "android-emulator-automation",
  "version": "0.1.0",
  "description": "Node.js service for automating browser interactions on Android emulators via ADB and Chrome DevTools Protocol",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "adb-auto": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^3.3.8",
    "chrome-remote-interface": "^0.34.0",
    "commander": "^14.0.3",
    "pino": "^10.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.4",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.4.1"
}
